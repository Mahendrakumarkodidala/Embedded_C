void Wait_for_input (void);

int main()
{
	uint8_t studentAge[100];
	printf("Size of an array =%d\n",sizeof(studentAge));
	Wait_for_input();
	return 0;
}











void Wait_for_input (void)
{
	printf("\n\nPress Enter key to exit\n");
	while(getchar() != '\n'){

	};
	getchar();
}
