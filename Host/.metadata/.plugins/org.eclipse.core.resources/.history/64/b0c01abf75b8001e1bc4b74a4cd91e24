#include <stdio.h>
#include <stdint.h>



void Wait_for_input (void);

int main()
{
	uint32_t data = 10;

	printf("value1 = %d\n",data);

	data = 50;

	printf("value1 = %d\n",data);

	Wait_for_input();
	return 0;
}

void Wait_for_input (void)
{
	printf("\n\nPress Enter Key to Exit!!\n");
	while(getchar()!='\n'){
		;
	}
	getchar();
}
